{
  "name": "aistairshepherd",
  "version": "2.0.0-alpha.1",
  "description": "",
  "browserslist": [
    "last 1 Chrome versions",
    "last 1 Firefox versions",
    "last 1 Safari versions",
    "last 1 Edge versions"
  ],
  "author": "Alistair Shepherd <alistair@accudio.com>",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "#"
  },
  "scripts": {
    "clean": "run-p clean:*",
    "clean:cache": "rimraf ./.cache",
    "clean:dist": "rimraf ./dist",
    "clean:bundle": "rimraf ./src/_includes/bundle",
    "compile": "run-s clean && run-p compile:assets compile:icons && run-s compile:html",
    "compile:html": "cross-env ELEVENTY_ENV=development eleventy",
    "compile:assets": "parcel build ./src/assets/scss/**/[^_]*.scss ./src/assets/js/*.js ./src/images/**/* -d src/_includes/bundle --no-minify",
    "compile:icons": "gulp",
    "build": "run-s clean && run-p build:assets build:icons && run-s build:html",
    "build:html": "cross-env ELEVENTY_ENV=production eleventy",
    "build:icons": "gulp",
    "build:assets": "parcel build ./src/assets/scss/**/[^_]*.scss ./src/assets/js/*.js ./src/images/**/* -d src/_includes/bundle --no-source-maps",
    "watch": "run-s clean compile:assets compile:icons && run-p watch:*",
    "watch:html": "cross-env ELEVENTY_ENV=development eleventy --serve --port=8000",
    "watch:icons": "gulp watch",
    "watch:assets": "parcel watch ./src/assets/scss/**/[^_]*.scss ./src/assets/js/*.js ./src/images/**/* -d src/_includes/bundle --no-source-maps --no-hmr",
    "dev": "npm run watch",
    "dist": "npm run build",
    "test": "npm run lint",
    "update:cssdb": "npx browserslist@latest --update-db",
    "lint": "run-p lint:*",
    "lint:js": "eslint --ignore-path .gitignore \"src/assets/js/**/*.js\"",
    "lint:css": "stylelint --ignore-path .gitignore \"src/assets/scss/**/*.scss\"",
    "fix": "run-p fix:*",
    "fix:js": "eslint --ignore-path .gitignore \"src/assets/js/**/*.js\" --fix",
    "fix:css": "stylelint --ignore-path .gitignore \"src/assets/scss/**/*.scss\" --fix"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-plugin-rss": "1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "3.0.1",
    "@babel/core": "7.10.4",
    "@babel/preset-env": "7.10.4",
    "@barba/core": "^2.9.7",
    "@barba/prefetch": "^2.1.10",
    "@plaiceholder/css": "^1.0.0",
    "@tbranyen/jsdom": "13.0.0",
    "animejs": "^3.2.0",
    "autoprefixer": "^9.8.5",
    "cross-env": "7.0.2",
    "cssnano": "4.1.10",
    "cssnano-preset-advanced": "4.0.7",
    "eleventy-plugin-pwa": "1.0.8",
    "gorko": "^0.2.1",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-svgmin": "^3.0.0",
    "gulp-svgstore": "^7.0.1",
    "html-minifier": "4.0.0",
    "markdown-it": "11.0.0",
    "node-sass": "^4.14.1",
    "node-sass-glob-importer": "^5.3.2",
    "npm-run-all": "4.1.5",
    "parcel": "^1.12.4",
    "postcss-clean": "^1.1.0",
    "postcss-focus-visible": "^5.0.0",
    "postcss-preset-env": "6.7.0",
    "postcss-reporter": "6.0.1",
    "promisify-4loc": "^1.0.0",
    "rimraf": "3.0.2",
    "sass": "^1.26.10",
    "sharp": "^0.26.3",
    "slugify": "1.4.4",
    "svgo": "^1.3.2"
  },
  "devDependencies": {
    "eslint": "^7.5.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "prettier": "2.0.5",
    "stylelint": "^13.6.1",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-scss": "^3.18.0"
  }
}
