{
  "name": "aistairshepherd",
  "version": "2.0.1",
  "description": "",
  "browserslist": [
    "last 1 Chrome versions",
    "last 1 Firefox versions",
    "last 1 Safari versions",
    "last 1 Edge versions"
  ],
  "author": "Alistair Shepherd <alistair@accudio.com>",
  "license": "UNLICENSED",
  "private": "true",
  "repository": {
    "type": "git",
    "url": "https://github.com/Accudio/alistair-shepherd/"
  },
  "scripts": {
    "clean": "rimraf ./.cache ./dist ./src/_includes/bundle/*.*",
    "compile": "run-s clean compile:assets compile:html",
    "compile:assets": "cross-env NODE_ENV=development gulp",
    "compile:html": "cross-env ELEVENTY_ENV=development eleventy",
    "build": "run-s clean build:assets build:html",
    "build:assets": "cross-env NODE_ENV=production gulp",
    "build:html": "cross-env ELEVENTY_ENV=production eleventy",
    "watch": "run-s clean compile:assets && run-p watch:*",
    "watch:assets": "cross-env NODE_ENV=development gulp watch",
    "watch:html": "cross-env ELEVENTY_ENV=development eleventy --serve --port=8000",
    "dev": "npm run watch",
    "dist": "npm run build",
    "test": "npm run lint",
    "update:cssdb": "npx browserslist@latest --update-db",
    "devimages": "npm run compile && surge dist/raw-images https://as-dev-images.surge.sh",
    "demo": "npm run dist && surge dist/ https://alistairshepherd.surge.sh",
    "subset-fonts": "rimraf static/fonts/*-subset.woff2 && glyphhanger --US_ASCII --subset=\"static/fonts/*.woff2\" --formats=woff2"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.3",
    "@barba/core": "^2.9.7",
    "@barba/prefetch": "^2.1.10",
    "@quasibit/eleventy-plugin-sitemap": "^2.1.4",
    "@tbranyen/jsdom": "^13.0.0",
    "autoprefixer": "^10.4.0",
    "cross-env": "^7.0.3",
    "gorko": "^0.9.0",
    "gulp": "^4.0.2",
    "gulp-asset-hash": "^1.0.1",
    "gulp-csso": "^4.0.1",
    "gulp-esbuild": "^0.9.2",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.0.0",
    "gulp-sass-glob": "^1.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-svgmin": "^4.1.0",
    "gulp-svgstore": "^9.0.0",
    "html-minifier": "^4.0.0",
    "markdown-it": "^12.2.0",
    "markdown-it-attrs": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.4",
    "promisify-4loc": "^1.0.0",
    "purgecss": "^4.1.3",
    "rimraf": "^3.0.2",
    "sass": "^1.44.0",
    "sharp": "^0.29.3",
    "svgo": "^2.8.0"
  },
  "devDependencies": {
    "@manustays/eleventy-plugin-codepen-iframe": "^2.0.1",
    "prettier": "^2.5.1"
  }
}
