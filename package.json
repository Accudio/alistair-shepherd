{
  "name": "aistairshepherd",
  "version": "2.0.0-alpha.1",
  "description": "",
  "browserslist": [
    "last 1 Chrome versions",
    "last 1 Firefox versions",
    "last 1 Safari versions",
    "last 1 Edge versions"
  ],
  "author": "Alistair Shepherd <alistair@accudio.com>",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "#"
  },
  "scripts": {
    "clean": "rimraf ./.cache ./dist ./src/_includes/bundle",
    "compile": "run-s clean compile:assets compile:html",
    "compile:assets": "cross-env NODE_ENV=development gulp",
    "compile:html": "cross-env ELEVENTY_ENV=development eleventy",
    "build": "run-s clean build:assets build:html",
    "build:assets": "cross-env NODE_ENV=production gulp",
    "build:html": "cross-env ELEVENTY_ENV=production eleventy",
    "watch": "run-s clean compile:assets && run-p watch:*",
    "watch:assets": "cross-env NODE_ENV=development gulp watch",
    "watch:html": "cross-env ELEVENTY_ENV=development eleventy --serve --port=8000",
    "dev": "npm run watch",
    "dist": "npm run build",
    "test": "npm run lint",
    "update:cssdb": "npx browserslist@latest --update-db",
    "lint": "run-p lint:*",
    "lint:js": "eslint --ignore-path .gitignore \"src/assets/js/**/*.js\"",
    "lint:css": "stylelint --ignore-path .gitignore \"src/assets/scss/**/*.scss\"",
    "fix": "run-p fix:*",
    "fix:js": "eslint --ignore-path .gitignore \"src/assets/js/**/*.js\" --fix",
    "fix:css": "stylelint --ignore-path .gitignore \"src/assets/scss/**/*.scss\" --fix",
    "devimages": "npm run compile && surge dist/raw-images https://as-dev-images.surge.sh"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-plugin-rss": "1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "3.0.1",
    "@barba/core": "^2.9.7",
    "@barba/prefetch": "^2.1.10",
    "@quasibit/eleventy-plugin-sitemap": "^2.0.3",
    "@tbranyen/jsdom": "13.0.0",
    "autoprefixer": "^9.8.5",
    "cross-env": "7.0.2",
    "gorko": "^0.2.1",
    "gulp": "^4.0.2",
    "gulp-csso": "^4.0.1",
    "gulp-esbuild": "^0.3.9",
    "gulp-postcss": "^9.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-svgmin": "^3.0.0",
    "gulp-svgstore": "^7.0.1",
    "html-minifier": "4.0.0",
    "markdown-it": "11.0.0",
    "node-sass": "^4.14.1",
    "node-sass-glob-importer": "^5.3.2",
    "npm-run-all": "4.1.5",
    "postcss": "^8.2.1",
    "promisify-4loc": "^1.0.0",
    "rimraf": "3.0.2",
    "sharp": "^0.27.0",
    "svgo": "^1.3.2"
  },
  "devDependencies": {
    "eslint": "^7.5.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "prettier": "2.0.5",
    "stylelint": "^13.6.1",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-scss": "^3.18.0"
  }
}
